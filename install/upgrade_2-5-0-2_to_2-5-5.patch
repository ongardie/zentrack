+
+<?
+function fmfRow( $text, $class ) {
+  print "<td class='$class'>{$text}</td>";
+}
+
+function fmfName( $field_name, $key ) {
+  return " name='{$field_name}[$key]' ";
+}
+
+function fmfVal( $field, $key ) {
+  global $zen;
+  return $zen->ffv($field[$key]);
+}
+
+$vprops = getFmViewProps($view);
+$fcount=0;
