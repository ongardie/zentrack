+    <b><?=tr("Edit field map for ?", $view)?></b>
+  </td>
+</tr>
+<?
+if( !is_array($fields) || !count($fields) || !$map || !$view ){
+  print "<tr><td colspan='10' class='cell'>This view has no properties to configure</td></tr>\n";
+}
+else {
+?>
+<tr toofar="toofar">
